  {{#canonical}}<link rel="canonical" href="{{{canonical}}}">{{/canonical}}
  {{#robots}}<meta name="robots" content="{{{robots}}}">{{/robots}}
  <link rel="icon" href="/assets/img/favicon.png" type="image/png">
